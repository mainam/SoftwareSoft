    <script>
        $("#btnChangePassword").click(function () {
            var target = "/account/dialogchangepassword.aspx"
            $('#remoteModal2').removeData();
            $('#remoteModal2').modal({ backdrop: 'static' });
            $('#remoteModal2').load(target, function () {
                $('#remoteModal2').modal("show");
            });
            return false;
        });

    </script>

